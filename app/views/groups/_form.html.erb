


<%= form_for group do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :body %>
    <%= f.text_area :body %>
  </div>

	<div>
		<%= f.label :youtube %>
		<%= f.text_area :youtube %>
	</div>

  <div>
    <%= f.label :attachments %>
    <%= f.file_field :attachments, multiple: true %>
  </div>

  <%= f.submit %>

<% end %>
